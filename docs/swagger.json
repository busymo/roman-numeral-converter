{
    "swagger": "2.0",
    "info": {
        "description": "API to convert a range of numbers to Roman numerals.",
        "title": "Roman Numeral API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/convert": {
            "get": {
                "parameters": [
                    {
                        "type": "string",
                        "description": "The range of numbers to convert, in the format 'from-to'",
                        "name": "range",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "type": "object",
                            "additionalProperties": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        }
    }
}